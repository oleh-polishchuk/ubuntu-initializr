[
  {
    "script": "app.js",
    "name": "u-i-worker",
    "exec_mode": "fork",
    "watch": true,
    "ignore_watch" : ["node_modules", "logs"],
    "watch_options": {
      "usePolling": true,
      "awaitWrite_finish": true,
      "followSymlinks": false,
      "interval": 2000,
      "binaryInterval": 5000
    },
    "autorestart": true,
    "restart_delay": 1000,
    "cwd": "./",
    "args": "['-c' '../conf/dev/config.json' '-p' '8080']",
    "node_args": ["--inspect=7000"],
    "error_file": "./logs/error.log",
    "out_file": "./logs/out.log"
  }
]